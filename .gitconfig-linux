[core]
  editor = /usr/bin/code >/dev/null 2>&1 --wait
  excludesfile = ~/.gitignore
[credential]
  helper = /mnt/c/Program\\ Files/Git/mingw64/bin/git-credential-manager.exe
[diff]
  tool = vscode
[difftool "vscode"]
  cmd = /usr/bin/code >/dev/null 2>&1 --wait --diff $LOCAL $REMOTE
[merge]
  tool = vscode
[mergetool "vscode"]
  cmd = /usr/bin/code >/dev/null 2>&1 --wait $MERGED
